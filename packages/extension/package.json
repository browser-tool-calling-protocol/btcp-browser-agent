{
  "name": "@aspect/extension",
  "version": "0.1.0",
  "description": "Chrome extension bridge for browser automation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./content": {
      "types": "./dist/content.d.ts",
      "import": "./dist/content.js"
    },
    "./background": {
      "types": "./dist/background.d.ts",
      "import": "./dist/background.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "chrome-extension",
    "browser-automation",
    "dom"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@aspect/core": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
